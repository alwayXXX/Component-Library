{
  "name": "ui-alwayxx",
  "type": "module",
  "version": "1.0.0",
  "description": "UI invisibili alwayxx",
  "author": "alwayxx",
  "license": "MIT",
  "homepage": "https://component-library-h8nqx5edq-alwayxxs-projects.vercel.app/",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/alwayxx-ui.js",
      "require": "./dist/alwayxx-umd.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/alwayxx-ui.umd.cjs",
  "module": "./dist/alwayxx-ui.js",
  "types": "./dist/entry.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:all": "vite build",
    "build:components": "tsx ./scripts/build.ts",
    "build": "pnpm build:components && pnpm type:build",
    "typecheck": "vue-tsc --noEmit",
    "type:build": "vue-tsc -b --declaration --emitDeclarationOnly tsconfig.types.json",
    "clean:dist": "rimraf dist"
  },
  "dependencies": {
    "alwayxx-ui-vite": "^1.1.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-istanbul": "^2.1.4",
    "@vue/test-utils": "2.4.6",
    "fs-extra": "^11.2.0",
    "happy-dom": "15.7.4",
    "typescript": "5.5.4",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vitest": "2.1.4",
    "vue-tsc": "^2.1.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
